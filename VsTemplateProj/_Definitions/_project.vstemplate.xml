    <Assembly>JumpStreetMobile, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null</Assembly>
    <FullClassName>JumpStreetMobile.VSSolutionWizard</FullClassName>
  </WizardExtension>
</VSTemplate>