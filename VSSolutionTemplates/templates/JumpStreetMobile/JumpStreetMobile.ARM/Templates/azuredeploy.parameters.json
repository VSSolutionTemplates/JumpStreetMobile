{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "JumpStreetMobile" // Must be a valid and unique Azure App Service name
    },
    "sqlServerAdminLogin": {
      "value": "" // Required value
    },
    "sqlServerAdminPassword": {
      "value": "" // Required value
    },
    "isAuthenticationEnabled": {
      "value": false
    },
    "microsoftAuth": {
      "value": {
        "clientId": "",
        "clientSecret": ""
      }
    },
    "googleAuth": {
      "value": {
        "googleClientId": "",
        "googleClientSecret": ""
      }
    },
    "facebookAuth": {
      "value": {
        "facebookAppId": "",
        "facebookAppSecret": ""
      }
    },
    "twitterAuth": {
      "value": {
        "twitterConsumerKey": "",
        "twitterConsumerSecret": ""
      }
    },
    "aadAuth": {
      "value": {
        "clientId": "",
        "issuer": ""
      }
    },

    // Notification Hub Credentials (for Windows, Google, and Apple push notification services)
    "wnsCredential": {
      "value": {
        "packageSid": "", // If you set these credentials you have to uncomment wnsCredential in Notification Hub configuration section
        "secretKey": ""
      }
    },
    "gcmCredential": {
      "value": {
        "googleApiKey": "" // If you set these credentials you have to uncomment gcmCredential in Notification Hub configuration section
      }
    },
    "apnsCredential": {
      "value": {
        // Thumbnail of .p12 cert which you can get by running: Get-PfxCertificate -filepath <path to .p12 file>

        "apnsCertificate": "", // If you set these credentials you have to uncomment apnsCredential in Notification Hub configuration section
        "certificateKey": "",
        "endpoint": ""
      }
    }
  }
}
